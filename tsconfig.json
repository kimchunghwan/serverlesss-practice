{
    "compilerOptions": {
      "lib": ["es2019"],
      "removeComments": true,
      "moduleResolution": "node",
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "sourceMap": true,
      "target": "es2017",
      "outDir": "lib"
    },
    "include": ["./**/*.ts"],
    "exclude": [
      "node_modules/**/*",
      ".serverless/**/*",
      ".webpack/**/*",
      "_warmup/**/*",
      ".vscode/**/*"
    ]
  }